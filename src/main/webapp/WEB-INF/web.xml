<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>WorkBookServlet</servlet-name>
        <servlet-class>com.pxqngu.workbook.WorkBookServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WorkBookServlet</servlet-name>
        <url-pattern>/workbook.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WorkBookDetailsServlet</servlet-name>
        <servlet-class>com.pxqngu.workbook.WorkBookDetailsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WorkBookDetailsServlet</servlet-name>
        <url-pattern>/workbookdetails.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>com.pxqngu.workbook.FileUploadServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/fileUpload.do</url-pattern>
    </servlet-mapping>
</web-app>